  <div class="glass-card metallic-border rounded-2xl overflow-hidden carbon-panel">
    <div class="steel-header px-6 py-4 flex items-center gap-3">
      <div class="feature-icon w-11 h-11 rounded-xl grid place-items-center">
        <svg class="w-5 h-5 text-volt-400" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M12 3v17.25m0 0c-1.472 0-2.882.265-4.185.75M12 20.25c1.472 0 2.882.265 4.185.75M18.75 4.97A48.416 48.416 0 0012 4.5c-2.291 0-4.545.16-6.75.47m13.5 0c1.01.143 2.01.317 3 .52m-3-.52l2.62 10.726c.122.499-.106 1.028-.589 1.202a5.989 5.989 0 01-2.031.352 5.989 5.989 0 01-2.031-.352c-.483-.174-.711-.703-.59-1.202L18.75 4.971zm-16.5.52c.99-.203 1.99-.377 3-.52m0 0l2.62 10.726c.122.499-.106 1.028-.589 1.202a5.989 5.989 0 01-2.031.352 5.989 5.989 0 01-2.031-.352c-.483-.174-.711-.703-.59-1.202L5.25 4.971z"/></svg>
      </div>
      <h2 class="font-bold text-white text-lg">{{ state.name }} Lemon Law</h2>
    </div>
    <div class="px-6 py-5 space-y-4">
      <p class="text-silver-400 text-sm leading-relaxed">{{ state.lemonLawSummary }}</p>
      <div class="flex flex-wrap gap-3 pt-2">
        <a href="/states/{{ state.slug }}/lemon-law/" class="inline-flex items-center gap-2 px-4 py-2.5 rounded-xl btn-ghost text-volt-400 text-sm font-medium">
          Full lemon law guide
          <svg class="w-3.5 h-3.5" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2"><path stroke-linecap="round" stroke-linejoin="round" d="M13.5 4.5L21 12m0 0l-7.5 7.5M21 12H3"/></svg>
        </a>
        <a href="/go/lemon-law/" target="_blank" rel="nofollow sponsored noopener" class="inline-flex items-center gap-2 px-4 py-2.5 rounded-xl btn-primary text-white text-sm font-bold">
          Talk to a lemon law attorney
          <svg class="w-3.5 h-3.5" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2"><path stroke-linecap="round" stroke-linejoin="round" d="M13.5 6H5.25A2.25 2.25 0 003 8.25v10.5A2.25 2.25 0 005.25 21h10.5A2.25 2.25 0 0018 18.75V10.5m-10.5 6L21 3m0 0h-5.25M21 3v5.25"/></svg>
        </a>
      </div>
    </div>
  </div>
</section>
{% endif %}

{# ── Emissions Testing Card ── #}
{% if state.hasEmissions %}
<section class="mb-8">
  <div class="glass-card metallic-border rounded-2xl overflow-hidden carbon-panel">
    <div class="steel-header px-6 py-4 flex items-center gap-3">
      <div class="feature-icon w-11 h-11 rounded-xl grid place-items-center">
        <svg class="w-5 h-5 text-volt-400" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M12 9v3.75m9-.75a9 9 0 11-18 0 9 9 0 0118 0zm-9 3.75h.008v.008H12v-.008z"/></svg>
      </div>
      <h2 class="font-bold text-white text-lg">Emissions Testing</h2>
    </div>
    <div class="px-6 py-5">
      <p class="text-silver-400 text-sm leading-relaxed">{{ state.emissionsDetails }}</p>
    </div>
  </div>
</section>
{% endif %}

{# ── Safety Inspection Card ── #}
{% if state.hasSafetyInspection %}
<section class="mb-8">
  <div class="glass-card metallic-border rounded-2xl overflow-hidden carbon-panel">
    <div class="steel-header px-6 py-4 flex items-center gap-3">
      <div class="feature-icon w-11 h-11 rounded-xl grid place-items-center">
        <svg class="w-5 h-5 text-volt-400" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M11.42 15.17l-5.1-5.1a1.5 1.5 0 010-2.12l.87-.87a1.5 1.5 0 012.12 0L12 9.75l2.69-2.69a1.5 1.5 0 012.12 0l.87.87a1.5 1.5 0 010 2.12l-5.1 5.1a1.5 1.5 0 01-2.12 0z"/></svg>
      </div>
      <h2 class="font-bold text-white text-lg">Safety Inspection</h2>
    </div>
    <div class="px-6 py-5">
      <p class="text-silver-400 text-sm leading-relaxed">{{ state.safetyInspectionDetails }}</p>
    </div>
  </div>
</section>
{% endif %}

{# ── EV Incentives Card ── #}
{% if state.evIncentives %}
<section class="mb-8">
  <div class="glass-card metallic-border rounded-2xl overflow-hidden carbon-panel">
    <div class="steel-header px-6 py-4 flex items-center gap-3">
      <div class="feature-icon w-11 h-11 rounded-xl grid place-items-center">
        <svg class="w-5 h-5 text-volt-400" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M3.75 13.5l10.5-11.25L12 10.5h8.25L9.75 21.75 12 13.5H3.75z"/></svg>
      </div>
      <h2 class="font-bold text-white text-lg">EV Incentives</h2>
    </div>
    <div class="px-6 py-5 space-y-4">
      <p class="text-silver-400 text-sm leading-relaxed">{{ state.evIncentiveDetails }}</p>
      <a href="/states/{{ state.slug }}/ev-incentives/" class="inline-flex items-center gap-2 px-4 py-2.5 rounded-xl btn-ghost text-volt-400 text-sm font-medium">
        Full EV incentives guide
        <svg class="w-3.5 h-3.5" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2"><path stroke-linecap="round" stroke-linejoin="round" d="M13.5 4.5L21 12m0 0l-7.5 7.5M21 12H3"/></svg>
      </a>
    </div>
  </div>
</section>
{% endif %}

{# ── Registration Info Card ── #}
<section class="mb-8">
  <div class="glass-card metallic-border rounded-2xl overflow-hidden carbon-panel">
    <div class="steel-header px-6 py-4 flex items-center gap-3">
      <div class="feature-icon w-11 h-11 rounded-xl grid place-items-center">
        <svg class="w-5 h-5 text-volt-400" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M19.5 14.25v-2.625a3.375 3.375 0 00-3.375-3.375h-1.5A1.125 1.125 0 0113.5 7.125v-1.5a3.375 3.375 0 00-3.375-3.375H8.25m0 12.75h7.5m-7.5 3H12M10.5 2.25H5.625c-.621 0-1.125.504-1.125 1.125v17.25c0 .621.504 1.125 1.125 1.125h12.75c.621 0 1.125-.504 1.125-1.125V11.25a9 9 0 00-9-9z"/></svg>
      </div>
      <h2 class="font-bold text-white text-lg">Registration &amp; Title</h2>
    </div>
    <div class="px-6 py-5 space-y-4">
      <div class="grid sm:grid-cols-3 gap-4">
        <div class="p-4 rounded-xl bg-white/[0.02] border border-white/[0.05]">
          <div class="text-[10px] font-semibold text-silver-500 uppercase tracking-wider">Registration Fee</div>
          <div class="font-bold text-white mt-1">{{ state.registrationFeeRange }}</div>
        </div>
        <div class="p-4 rounded-xl bg-white/[0.02] border border-white/[0.05]">
          <div class="text-[10px] font-semibold text-silver-500 uppercase tracking-wider">Title Fee</div>
          <div class="font-bold text-white mt-1">{{ state.titleFee }}</div>
        </div>
        <div class="p-4 rounded-xl bg-white/[0.02] border border-white/[0.05]">
          <div class="text-[10px] font-semibold text-silver-500 uppercase tracking-wider">Plate Fee</div>
          <div class="font-bold text-white mt-1">{{ state.plateFee }}</div>
        </div>
      </div>
      <p class="text-silver-400 text-sm leading-relaxed">{{ state.registrationNotes }}</p>
      <a href="/states/{{ state.slug }}/registration/" class="inline-flex items-center gap-2 px-4 py-2.5 rounded-xl btn-ghost text-volt-400 text-sm font-medium">
        Full registration guide
        <svg class="w-3.5 h-3.5" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2"><path stroke-linecap="round" stroke-linejoin="round" d="M13.5 4.5L21 12m0 0l-7.5 7.5M21 12H3"/></svg>
      </a>
    </div>
  </div>
</section>

{# ── Topic Sub-Pages ── #}
<section class="mb-8">
  <h2 class="text-xl font-bold text-white mb-5">{{ state.name }} Vehicle Ownership Topics</h2>
  <div class="grid sm:grid-cols-2 gap-4">
    <a href="/states/{{ state.slug }}/insurance/" class="glass-card metallic-border rounded-2xl p-5 carbon-panel card-hover flex items-start gap-3 group">
      <svg class="w-5 h-5 text-volt-500 mt-0.5 shrink-0" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M9 12.75L11.25 15 15 9.75m-3-7.036A11.959 11.959 0 013.598 6 11.99 11.99 0 003 9.749c0 5.592 3.824 10.29 9 11.623 5.176-1.332 9-6.03 9-11.622 0-1.31-.21-2.571-.598-3.751h-.152c-3.196 0-6.1-1.248-8.25-3.285z"/></svg>
      <div>
        <div class="font-bold text-white group-hover:text-volt-400 transition-colors">Car Insurance</div>
        <div class="text-xs text-silver-500 mt-1">Requirements, average costs, and tips to save</div>
      </div>
    </a>
    <a href="/states/{{ state.slug }}/lemon-law/" class="glass-card metallic-border rounded-2xl p-5 carbon-panel card-hover flex items-start gap-3 group">
      <svg class="w-5 h-5 text-volt-500 mt-0.5 shrink-0" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M12 3v17.25m0 0c-1.472 0-2.882.265-4.185.75M12 20.25c1.472 0 2.882.265 4.185.75"/></svg>
      <div>
        <div class="font-bold text-white group-hover:text-volt-400 transition-colors">Lemon Law</div>
        <div class="text-xs text-silver-500 mt-1">Your rights and how to file a claim</div>
      </div>
    </a>
    <a href="/states/{{ state.slug }}/registration/" class="glass-card metallic-border rounded-2xl p-5 carbon-panel card-hover flex items-start gap-3 group">
      <svg class="w-5 h-5 text-volt-500 mt-0.5 shrink-0" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M19.5 14.25v-2.625a3.375 3.375 0 00-3.375-3.375h-1.5A1.125 1.125 0 0113.5 7.125v-1.5a3.375 3.375 0 00-3.375-3.375H8.25m0 12.75h7.5m-7.5 3H12M10.5 2.25H5.625c-.621 0-1.125.504-1.125 1.125v17.25c0 .621.504 1.125 1.125 1.125h12.75c.621 0 1.125-.504 1.125-1.125V11.25a9 9 0 00-9-9z"/></svg>
      <div>
        <div class="font-bold text-white group-hover:text-volt-400 transition-colors">Registration &amp; Title</div>
        <div class="text-xs text-silver-500 mt-1">Fees, process, and required documents</div>
      </div>
    </a>
    {% if state.evIncentives %}
    <a href="/states/{{ state.slug }}/ev-incentives/" class="glass-card metallic-border rounded-2xl p-5 carbon-panel card-hover flex items-start gap-3 group">
      <svg class="w-5 h-5 text-volt-500 mt-0.5 shrink-0" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M3.75 13.5l10.5-11.25L12 10.5h8.25L9.75 21.75 12 13.5H3.75z"/></svg>
      <div>
        <div class="font-bold text-white group-hover:text-volt-400 transition-colors">EV Incentives</div>
        <div class="text-xs text-silver-500 mt-1">State and federal credits for electric vehicles</div>
      </div>
    </a>
    {% endif %}
  </div>
</section>

{# ── Bottom CTA ── #}
<div class="mt-12 glass-card metallic-border rounded-2xl p-8 carbon-panel text-center">
  <h3 class="text-xl font-bold text-white mb-3">Compare Insurance Rates in {{ state.name }}</h3>
  <p class="text-silver-400 mb-6 max-w-lg mx-auto">See how much you could save by comparing rates from top carriers in {{ state.name }}. It takes less than 5 minutes.</p>
