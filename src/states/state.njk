---
layout: layouts/page.njk
pagination:
  data: states
  size: 1
  alias: state
permalink: "/states/{{ state.slug }}/"
eleventyComputed:
  title: "Vehicle Ownership in {{ state.name }} — Car Alpha"
  description: "Complete guide to vehicle ownership in {{ state.name }}: insurance requirements ({{ state.insuranceMinimum }}), lemon law protections, emissions testing, registration fees, and EV incentives."
  breadcrumbs:
    - label: Home
      url: /
    - label: States
      url: /states/
    - label: "{{ state.name }}"
---

<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "Article",
  "headline": "Vehicle Ownership in {{ state.name }}",
  "description": "{{ description }}",
  "author": {
    "@type": "Organization",
    "name": "Car Alpha"
  },
  "publisher": {
    "@type": "Organization",
    "name": "Car Alpha",
    "logo": {
      "@type": "ImageObject",
      "url": "https://www.caralpha.com/assets/images/logo.png"
    }
  },
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": "https://www.caralpha.com/states/{{ state.slug }}/"
  }
}
</script>

{# ── State Overview ── #}
<section class="mb-10">
  <div class="glass-card metallic-border rounded-2xl overflow-hidden carbon-panel">
    <div class="steel-header px-6 py-4 flex items-center gap-3">
      <div class="feature-icon w-11 h-11 rounded-xl grid place-items-center">
        <svg class="w-5 h-5 text-volt-400" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M15 10.5a3 3 0 11-6 0 3 3 0 016 0z"/><path stroke-linecap="round" stroke-linejoin="round" d="M19.5 10.5c0 7.142-7.5 11.25-7.5 11.25S4.5 17.642 4.5 10.5a7.5 7.5 0 1115 0z"/></svg>
      </div>
      <div>
        <h2 class="font-bold text-white text-lg">{{ state.name }} Overview</h2>
        <span class="text-xs text-silver-500">{{ state.region }} &middot; Capital: {{ state.capital }}</span>
      </div>
    </div>
    <div class="px-6 py-5">
      <p class="text-silver-400 leading-relaxed">{{ state.description }}</p>
    </div>
  </div>
</section>

{# ── Insurance Requirements Card ── #}
<section class="mb-8">
  <div class="glass-card metallic-border rounded-2xl overflow-hidden carbon-panel">
    <div class="steel-header px-6 py-4 flex items-center gap-3">
      <div class="feature-icon w-11 h-11 rounded-xl grid place-items-center">
        <svg class="w-5 h-5 text-volt-400" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5"><path stroke-linecap="round" stroke-linejoin="round" d="M9 12.75L11.25 15 15 9.75m-3-7.036A11.959 11.959 0 013.598 6 11.99 11.99 0 003 9.749c0 5.592 3.824 10.29 9 11.623 5.176-1.332 9-6.03 9-11.622 0-1.31-.21-2.571-.598-3.751h-.152c-3.196 0-6.1-1.248-8.25-3.285z"/></svg>
      </div>
      <h2 class="font-bold text-white text-lg">Insurance Requirements</h2>
    </div>
    <div class="px-6 py-5 space-y-4">
      <div class="grid sm:grid-cols-2 gap-4">
        <div class="p-4 rounded-xl bg-white/[0.02] border border-white/[0.05]">
          <div class="text-[10px] font-semibold text-silver-500 uppercase tracking-wider">Minimum Coverage</div>
          <div class="font-bold text-white text-lg mt-1">{{ state.insuranceMinimum }}</div>
        </div>
        {% if state.averageInsuranceCost %}
        <div class="p-4 rounded-xl bg-white/[0.02] border border-white/[0.05]">
          <div class="text-[10px] font-semibold text-silver-500 uppercase tracking-wider">Average Annual Cost</div>
          <div class="font-bold text-white text-lg mt-1">${{ state.averageInsuranceCost | toLocaleString }}</div>
        </div>
        {% endif %}
      </div>
      <p class="text-silver-400 text-sm leading-relaxed">{{ state.insuranceMinimumDetail }}</p>
      <div class="flex flex-wrap gap-3 pt-2">
        <a href="/states/{{ state.slug }}/insurance/" class="inline-flex items-center gap-2 px-4 py-2.5 rounded-xl btn-ghost text-volt-400 text-sm font-medium">
          Full insurance guide
          <svg class="w-3.5 h-3.5" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2"><path stroke-linecap="round" stroke-linejoin="round" d="M13.5 4.5L21 12m0 0l-7.5 7.5M21 12H3"/></svg>
        </a>
        <a href="/go/insurance/" target="_blank" rel="nofollow sponsored noopener" class="inline-flex items-center gap-2 px-4 py-2.5 rounded-xl btn-primary text-white text-sm font-bold">
          Compare {{ state.name }} rates
          <svg class="w-3.5 h-3.5" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2"><path stroke-linecap="round" stroke-linejoin="round" d="M13.5 6H5.25A2.25 2.25 0 003 8.25v10.5A2.25 2.25 0 005.25 21h10.5A2.25 2.25 0 0018 18.75V10.5m-10.5 6L21 3m0 0h-5.25M21 3v5.25"/></svg>
        </a>
      </div>
    </div>
  </div>
</section>

{# ── Lemon Law Card ── #}
{% if state.hasLemonLaw %}
